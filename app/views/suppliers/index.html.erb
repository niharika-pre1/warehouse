<h1>SUPPLIERS</h1>
 
 <table>
  <tr>
    <th>Name</th>
    <th>Mobile</th>
    <th>Product_quantity</th>
    <th>product_price</th>
    
    <th colspan="3"></th>
  </tr>
 
  <% @suppliers.each do |supplier| %>
    <tr>
      <td><%= supplier.name %></td>
      <td><%= supplier.mobile_no %></td>
      <td><%= supplier.product_quantity %></td>
      <td><%= supplier.product_price %></td>


       <td><%= link_to 'Show', supplier_path(supplier) %></td>
    
    </tr>
  <% end %>
</table>

 
 

